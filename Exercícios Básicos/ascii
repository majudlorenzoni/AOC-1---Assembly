# As letras MAISCULAS começam em 64
# As letras minusculas comecam em 96
.data
palavra: .asciiz "Ola"

.text 

lb $a0, palavra($zero)
li $v0, 1
syscall

li $t0, 1
lb $a0, palavra($t0)
li $v0, 1
syscall

li $t0, 2
lb $a0, palavra($t0)
li $v0, 1
syscall
#apareceu 79, pois é o número de O na tabela ASCII
#
