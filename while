.data
	espaco: .byte ' ' #variavel para que os numeros sejam colocados em tela de forma espaçada
.text
	li $v0, 5
	syscall
	
	move $t0, $v0  #valor lido pra direcioinado para $t0
	move $t1, $zero
	
	laco:
	bgt $t1, $t0, saida
	#imprimir $t1
	li $v0, 1 
	move $a0, $t1
	syscall
	
	#imprimir espaco em branco
	li $v0, 4
	la $a0, espaco
	syscall
	
	addi $t1, $t1, 1
	j laco
	
	saida:
	
	
	